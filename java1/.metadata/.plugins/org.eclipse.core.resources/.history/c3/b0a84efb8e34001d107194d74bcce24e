package ch05;

import javax.swing.JFrame;

public class MyFrame6 extends JFrame {

    public MyFrame6() {
        initData();
        setInitLayout();
        addEventListener();
    }

    private void initData() {
        setTitle("어댑터 클래스 이용");
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        setSize(500, 500);
    }
    private void setInitLayout() {
        setVisible(true);
        setResizable(false);
    }
    private void addEventListener() {

    }
}