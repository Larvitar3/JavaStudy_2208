package ch05;

import javax.swing.JFrame;

public class MyFrame7 extends JFrame {

	public MyFrame7() {
	        initData();
	        setInitLayout();
	        addEventListener();
	    }

	private void initData() {
		setTitle("키보드 이벤트 연습");
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setSize(500, 500);
	}

	private void setInitLayout() {
		setVisible(true);
		setResizable(false);
	}

	private void addEventListener() {

	}

}
