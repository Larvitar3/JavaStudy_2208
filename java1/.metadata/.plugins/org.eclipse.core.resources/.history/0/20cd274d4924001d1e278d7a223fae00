package ch01;

public class MainTest {

	public static void main(String[] args) {
		
		char ch1 = 'A';
		System.out.println(ch1);
		
		// 형변환 (데이터 타입 변환)
		System.out.println((int)ch1);
		
		char ch2 = 'F';
		System.out.println((int)ch2);
		
		char ch3 = 66;
		System.out.println((char)ch3);
		

	}

}
