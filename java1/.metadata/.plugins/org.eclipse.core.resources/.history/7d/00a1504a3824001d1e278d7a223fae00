package ch01;

public class Variable {

	// 코드의 시작점
	public static void main(String[] args) {
		// 변수를 사용하는 방법
		// 변수는 변하는 수
		int age; // 변수 선언	
		int count; 
		
		age = 1; // ★ [=] (대입연산자) 오른쪽에서 왼쪽으로 연산한다.
					  // 초기화 (값을 넣다)
		count = 100;
		
		System.out.println(age);
		System.out.println(count);

	} // end of main

} // end of class
